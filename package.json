{
  "name": "machinepack-dictionaries",
  "version": "2.2.0",
  "description": "Work with dictionaries.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "Dictionaries",
    "machines",
    "machinepack"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "browserify-transform-machinepack": "^1.0.0",
    "lodash": "^3.7.0",
    "machine": "^10.3.1"
  },
  "devDependencies": {
    "test-machinepack-mocha": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "browserify-transform-machinepack"
    ]
  },
  "machinepack": {
    "friendlyName": "Dictionaries",
    "machineDir": "machines/",
    "machines": [
      "merge",
      "construct",
      "dot",
      "delete-key",
      "rename-key",
      "copy-key",
      "add-key",
      "keys"
    ],
    "testsUrl": "https://travis-ci.org/treelinehq/machinepack-dictionaries"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:treelinehq/machinepack-dictionaries.git"
  }
}
